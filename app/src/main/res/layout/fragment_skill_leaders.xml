<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/FrameLayoutSkillIq"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.SkillLeadersFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewSkillIq"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/item_skill_iq"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>

    <fr.castorflex.android.circularprogressbar.CircularProgressBar
        android:id="@+id/pb_skill_leaders"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:indeterminate="true"
        android:visibility="gone"
        app:cpb_color="#FFee44"
        app:cpb_colors="@array/mycolors"
        app:cpb_max_sweep_angle="300"
        app:cpb_min_sweep_angle="10"
        app:cpb_rotation_speed="2.5"
        app:cpb_stroke_width="5dp"
        app:cpb_sweep_speed="2.0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>